{
  "name": "trader-note-build-ai",
  "version": "1.0.0",
  "description": "TradeAssist MVP - Trade note generation and market matching system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run build:frontend",
    "build:frontend": "cd src/frontend && npm run build",
    "start": "node dist/index.js",
    "dev": "concurrently \"BACKEND_PORT=3100 nodemon --exec ts-node src/index.ts\" \"cd src/frontend && npm run dev\"",
    "dev:backend": "BACKEND_PORT=3100 nodemon --exec ts-node src/index.ts",
    "dev:frontend": "cd src/frontend && npm run dev",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:format": "prisma format",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.3",
    "concurrently": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prisma": "^5.22.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
