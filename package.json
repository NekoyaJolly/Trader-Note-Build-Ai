{
  "name": "trader-note-build-ai",
  "version": "1.0.0",
  "description": "TradeAssist - ノート主体のインテリジェント取引支援システム",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run build:frontend",
    "build:backend": "npx prisma generate && tsc",
    "build:frontend": "cd src/frontend && npm run build",
    "start": "node dist/index.js",
    "kill:ports": "lsof -ti :3100 | xargs kill -9 2>/dev/null; lsof -ti :3102 | xargs kill -9 2>/dev/null; echo 'Ports cleared'",
    "dev": "npm run kill:ports && concurrently \"BACKEND_PORT=3100 nodemon src/index.ts\" \"cd src/frontend && npm run dev\"",
    "dev:backend": "npm run kill:ports && BACKEND_PORT=3100 nodemon src/index.ts",
    "dev:frontend": "lsof -ti :3102 | xargs kill -9 2>/dev/null; cd src/frontend && npm run dev",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:format": "prisma format",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^6.0.0",
    "bottleneck": "^2.19.5",
    "bullmq": "^5.66.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "indicatorts": "^2.2.2",
    "jsonwebtoken": "^9.0.3",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/web-push": "^3.6.4",
    "concurrently": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prisma": "^5.22.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
